	<script>
	    window.onload = function() {
	        isIE9U();
	    };

	    function isIE9U() {
	        var ua = window.navigator.userAgent;
	        var ieVersion;
	        var msie = ua.indexOf('MSIE ');
	        if (msie > 0) {
	            // IE 10 or older => return version number
	            ieVersion = parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)),
	                    10);
	        }
	        if (typeof (ieVersion) != "undefined" && ieVersion <= 9) {
	            ie9UModalOpen();
	        }
	    }
	    function ie9UModalOpen() {
	        var url=window.location.toString();
	        var modal = $('#ie9URedirectModal');
	        var btn_continue = $('#btn_continue');
	        var btn_redirect = $('#btn_redirect');
	        var option = {
	            show : 'true',
	            backdrop : 'static'
	        };
	        modal.modal(option);
	        btn_continue.on('click', function() {
	            modal.modal('hide');
	        });
	        btn_redirect.on('click', function() {
	            window.location.href = url +'?isWeb=N';
	        });
	    }
        
	</script>
