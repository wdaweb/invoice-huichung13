 <script type="text/javascript">
 $(function(){
	 $('.dropdown').each(function() {
     $(this).children('a').focus(function() {
       $(".nav > .dropdown").removeClass("tabfocus open");
       $(this).parent('.dropdown').addClass("tabfocus open");
     });
	 });
	 $('.dropdown-menu').each(function() {
	   $(this).find('a').focus(function(){
	     $(this).css("background-color","#1da8fc");
	   });
	     $(this).find('a').blur(function(){
	         $(this).css("background-color","");
	     });
	 });
	 $('.dropdown-menu').last().find('a').last().blur(function(){
	   $(".nav > .dropdown").removeClass("tabfocus open");
	 });
	 $('.dropdown-item').each(function() {
	   $(this).children('a').focus(function() {
	     $(".nav > .dropdown").removeClass("tabfocus");
	         $(this).parents('.dropdown').addClass("tabfocus");
	     });
	 });
	 $('#header').focusin(function() {
	     $(".nav > .dropdown").removeClass("tabfocus");
	 });
	 $('.row').focusin(function() {
	   $(".nav > .dropdown").removeClass("tabfocus");
	 });
	
});
 </script>
</html>

