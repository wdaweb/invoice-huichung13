				function doSearch(text) {
					if ($('#gsc-i-id1').length > 0 && text != '') {
						$('#gsc-i-id1').css('background', '');
						$('#gsc-i-id1').val(text);
						$('input.gsc-search-button:button').trigger('click');
					}
				}

				$(window).load(function () {
					
					if ($('#gsc-i-id1').length > 0) {
						$('#gsc-i-id1').autocomplete({
			                source: function(request, response) {

			                    var inputText = $('#gsc-i-id1').val();

			                    var suggestQueriesURL = window.location.protocol + "//" + window.location.hostname
			                        + "/etwmain/front/ETW161W/promptSearch?text="
			                        + encodeURIComponent(inputText.trim());

			                    $.ajax({
			                        url: suggestQueriesURL,
			                        type:"GET",
			                        dataType:'json',
			                        success: function(data){
			                            if(data.promptState && data.promptState == '1'){
			                                var suggestions = [];
			                                $.each(data.promptWord, function(key, val) {
			                                    suggestions.push({"value":val});
			                                });
			                                if (suggestions.length > 10) {
			                                    suggestions.length = 10;
			                                }
			                                response(suggestions);
			                            }
			                        }
			                    });
			                },
			            });


			            /*google suggest queries*/
			            /*
			            $('#gsc-i-id1').autocomplete({
			                source: function(request, response) {
			                    var suggestQueriesURL = window.location.protocol
			                                          + "//suggestqueries.google.com/complete/search?callback=?";
			                    $.getJSON(suggestQueriesURL,
			                        {
			                          "hl":"zh-TW",
			                          "jsonp":"suggestCallBack",
			                          "q":request.term,
			                          "client":"youtube"
			                        }
			                    );
			                    suggestCallBack = function (data) {
			                        var suggestions = [];
			                        $.each(data[1], function(key, val) {
			                            suggestions.push({"value":val[0]});
			                        });
			                        if (suggestions.length > 10) {
	                                    suggestions.length = 10;
	                                }
			                        response(suggestions);
			                    };
			                },
			            });
			            */
					}

					if ($('.keywordsHref').length > 0) {
						$('.keywordsHref').bind("click", function (e) {
							e.preventDefault();
							doSearch($(this).attr('title'))
						});
					}

				});

				$(window).load(function () {

                    if ($('.keywordsHref').length > 0) {
