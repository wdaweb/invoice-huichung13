        ];
        if (typeof(buildDynamicMenuByJS) != 'undefined' && buildDynamicMenuByJS) {
            var homeText = '首頁';
            var firstLevelCrumbName = "";
            var secondLevelCrumbName = "";
            var aBreadCrumbs = $("div.content").find("a");
            if (aBreadCrumbs != null && aBreadCrumbs.length > 0) {
                for (var i=0; i<aBreadCrumbs.length; i++) {
                    if ($(aBreadCrumbs[i]).text() == homeText) {
                        if (i+1 < aBreadCrumbs.length) {
                            firstLevelCrumbName = $(aBreadCrumbs[(i+1)]).text();
                        }
                        if (i+2 < aBreadCrumbs.length) {
                            secondLevelCrumbName = $(aBreadCrumbs[(i+2)]).text();
                        }
                        break;
                    }
                }
            }

            var isFind = false;
            for (var i=0; i<navMenu.length; i++) {

                if (navMenu[i].display == 'Y' && navMenu[i].title == firstLevelCrumbName) {
                    isFind = true;
                    //build matched menu

                    

                    //$("div.panel-heading").text(firstLevelCrumbName);
                    $("#leftMenu").append("<li class='panel-body-header-bg'><a><i class='fa fa-home fa-fw space01'></i>" + firstLevelCrumbName + "</a></li>");
                    var subMenuList = navMenu[i].subMenu;
                    if (subMenuList != null) {
                        for (var j=0; j<subMenuList.length; j++) {
                            $("#leftMenu").append("<li><a href=\"" + subMenuList[j].link + "\" title=\"" + subMenuList[j].title + "\">" + subMenuList[j].title + "</a></li>");
                        }
                    }
                    break;
                }
            }
            if (isFind) {
                if (secondLevelCrumbName != '') {
                    $("#leftMenu li").removeClass("active");
                    $("#leftMenu li a[title='" + secondLevelCrumbName + "']").parent("li").addClass("active");
                } else {
                    if (firstLevelCrumbName != '') {
                        $("#leftMenu li").removeClass("active");
                        $("#leftMenu li a[title='" + firstLevelBreadCrumb + "']").parent("li").addClass("active");
                    }
                }
            } else {
                //build top level menu
                //$("div.panel-heading").text(homeText);
                $("#leftMenu").append("<li class='active'><a><i class='fa fa-home fa-fw space01'></i>" + homeText + "</a></li>");
                for (var i=0; i<navMenu.length; i++) {
                    if (navMenu[i].display == 'Y') {
                        $("#leftMenu").append("<li><a href=\"" + navMenu[i].link + "\" title=\"" + navMenu[i].title + "\">" + navMenu[i].title + "</a></li>");
                    }
                }
            }
        }
        
        
        $('#contentTitle').html('統一發票管理');
        
        
        if ($("#crumb").length > 0) {
        
        }
        //For original button of font size change handling
        $("div.font_change a").click(function() {
            var fontSize = 1.1;
            var fontSizePx = 15;
            if (this.className.indexOf('font01') >= 0) { fontSize = 1.0; fontSizePx = 14; }
            else if (this.className.indexOf('font03') >= 0) { fontSize = 1.2; fontSizePx = 17; }
            changeContentFontSize(fontSize, fontSizePx);
        });
        //For original button of print, back handling
        $("div.prgoback a").click(function() {
            if (this.className.indexOf('font01') >= 0) {
                window.print();
            } else if (this.className.indexOf('font02') >= 0) {
                $(this).attr("href", "javascript:void(0);");
                history.go(-1);
            }
        });

        //Original method from baseInclude >>
        try {
            if (top.location.hostname != window.location.hostname)
                top.location.href = window.location.href;
        } catch (e) {
            top.location.href = window.location.href;
        }
        
        
          //Get news by AJAX
          getBlockTypeLNews();
        
        //Set default content font-size
        changeContentFontSize(1.1, 15);
        //Button of font size change handling
        $("div.font-change button").click(function() {
            $(this).siblings('button').removeClass("active");
            var fontSize = 1.1;
            var fontSizePx = 15;
            if (this.className.indexOf('font-small') >= 0) { fontSize = 1.0; fontSizePx = 14; }
            else if (this.className.indexOf('font-large') >= 0) { fontSize = 1.2; fontSizePx = 17; }
            changeContentFontSize(fontSize, fontSizePx);
            $(this).addClass("active");
        });
        //Button of print, mail, back handling
        $("div.print-mail-back button").click(function() {
            if (this.className.indexOf('print') >= 0) {
                if(navigator.userAgent.toLowerCase().indexOf('chrome') > -1){   // Chrome Browser Detected?
				            	    window.PPClose = false;                                     // Clear Close Flag
				            	    window.onbeforeunload = function(){                         // Before Window Close Event
				            	        if(window.PPClose === false){                           // Close not OK?
				            	            return 'Leaving this page will block the parent window!\nPlease select "Stay on this Page option" and use the\nCancel button instead to close the Print Preview Window.\n';
				            	        }
				            	    }                   
				            	    window.print();                                             // Print preview
				            	    window.PPClose = true;                                      // Set Close Flag to OK.
				            	}
            } else if (this.className.indexOf('back') >= 0) {
                $(this).attr("href", "javascript:void(0);");
                history.go(-1);
            } else if (this.className.indexOf('mail') >= 0) {
                //openMailForwardDialog();
            }
        });
        //Button of top handling
        $('#top').click(function() {
            $('html, body').scrollTop(0);
        });
        //Tab change handling
        $('.nav-tabs a').click(function(e) {
            e.preventDefault();
            $(this).parent('li').siblings('li').removeClass("active");
            $(this).parent('li').addClass("active");
            var tabContentId = $(this).attr('href') + "_tab_content";
            var tabMenuId = $(this).attr('href') + "_tab_menu";
            $(tabContentId).siblings('div').removeClass("active");
            $(tabContentId).addClass("active");
            $(tabMenuId).siblings('div').removeClass("active");
            $(tabMenuId).addClass("active");
        });
        //Menu hover style handling
        $('.dropdown-menu_attach').hover(
            function() {
                $(this).parent('li').addClass("active");
            },
            function() {
                $(this).parent('li').removeClass("active");
            }
        )
        //Adjust privacy note message dialog's font size
        if ($("#privacyNoteMsg").length > 0) {
            $("#privacyNoteMsg").css("font-size", "1em");
        }

      //download?fname=ETWF001_20130507135358962.pdf&
         $("a[href*='download?fname=']").each(function() {
            var href = $(this).attr('href').match(/(download\?fname=)(.*)\.(.*)(\&+)/);
            if(href != null){
                $(this).attr('title',$(this).text() + '[' + href[3].toUpperCase() + ']' );
            }

        });
        //Original method from baseInclude <<
      });
    </script>
	
